#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int n; 
    scanf("%d",&n);
    int arr[n];
    for(int arr_i = 0; arr_i < n; arr_i++){
       scanf("%d",&arr[arr_i]);
    }
    
    int sum=0;  // declaring & initializing the variable to store the sum
    for(int i=0; i<n; i++){
        sum += arr[i];  // adding the elements in the array to the sum
    }
    
    printf("%d\n",sum); // printing the sum
    
    return 0;
}
